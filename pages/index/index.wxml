<view>
    <SearchInput></SearchInput>

    <swiper class="index-swiper" autoplay="{{true}}" interval="5000" indicator-dots="{{true}}">
        <swiper-item wx:for="{{swiperList}}" wx:key="goods_id">
            <navigator >
                <image mode="widthFix"src="{{item.image_src}}"/>
            </navigator>
        </swiper-item>
    </swiper>
</view>
